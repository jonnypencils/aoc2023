DIM I AS INT
DIM P AS INT
DIM HOPS AS LONG
DIM D AS BYTE
DIM FIN AS BYTE
DIM NEXTND AS STRING * 3
DIM DR AS STRING * 1 

DIM DEBUG AS STRING * 1 

DIM directions(4) AS STRING * 96 @lr
DIM nodedata(698) AS STRING * 9 @nodes

HOPS=0

FUNCTION nextnode AS STRING * 3 (dir AS STRING * 1, POS AS INT) STATIC
    IF dir ="L" THEN 
        RETURN MID$(nodedata(POS),3,3)
    ELSE 
        RETURN MID$(nodedata(POS),6,3)
    END IF
END FUNCTION

'Get start position
FOR I = 0 TO 697
    IF LEFT$(nodedata(I),3) = "AAA" THEN
        P=I
    END IF
NEXT I   

'Main direction loops

DO WHILE NEXTND <> "ZZZ"
        D = 0
        DR = "A"
        DO WHILE DR <> "X"
            DR=MID$(directions(0),D,1)
            NEXTND=nextnode(DR,P)
            ' NEXTND now equals the next string to find
            FOR I = 0 TO 697
                IF LEFT$(nodedata(I),3) = NEXTND AND NEXTND <> "ZZZ" THEN
                P=I
                HOPS=HOPS+1
                END IF    
                IF NEXTND = "ZZZ" THEN
                    EXIT DO
                END IF  
            NEXT I       
            D=D+1
        LOOP

        D = 0
        DR = "A"
        DO WHILE DR <> "X"
            DR=MID$(directions(1),D,1)
            NEXTND=nextnode(DR,P)
            ' NEXTND now equals the next string to find
            FOR I = 0 TO 697
                IF LEFT$(nodedata(I),3) = NEXTND AND NEXTND <> "ZZZ" THEN
                P=I
                HOPS=HOPS+1
                END IF
                IF NEXTND = "ZZZ" THEN
                    EXIT DO
                END IF  
            NEXT I          
            D=D+1
        LOOP

        D = 0
        DR = "A"
        DO WHILE DR <> "X"
            DR=MID$(directions(2),D,1)
            NEXTND=nextnode(DR,P)
            ' NEXTND now equals the next string to find
            FOR I = 0 TO 697
                IF LEFT$(nodedata(I),3) = NEXTND AND NEXTND <> "ZZZ" THEN
                P=I
                HOPS=HOPS+1
                END IF        
                IF NEXTND = "ZZZ" THEN
                    EXIT DO
                END IF  
            NEXT I          
            D=D+1
        LOOP

        D = 0
        DR = "A"
        DO WHILE DR <> "X"
            DR=MID$(directions(3),D,1)
            NEXTND=nextnode(DR,P)
            ' NEXTND now equals the next string to find
            FOR I = 0 TO 697
                IF LEFT$(nodedata(I),3) = NEXTND AND NEXTND <> "ZZZ" THEN
                P=I
                HOPS=HOPS+1
                END IF
                IF NEXTND = "ZZZ" THEN
                    EXIT DO
                END IF    
            NEXT I          
            D=D+1
        LOOP

LOOP ' main BIG loop

' We should have now exited all 4 loops
' Either by finding ZZZ of coming to the end of one of the the array string

PRINT ""
PRINT "the number of hops is : ";HOPS

